{
  "name": "obsidian-day-planner",
  "version": "0.6.0",
  "description": "A plugin to help you plan your day and setup pomodoro timers",
  "main": "main.js",
  "scripts": {
    "dev": "rollup --config rollup.config.js -w",
    "build": "rollup --config rollup.config.js",
    "test": "cross-env TS_NODE_COMPILER_OPTIONS='{ \"module\": \"commonjs\" }' mocha -r ts-node/register -r ignore-styles -r jsdom-global/register tests/**/*.test.ts",
    "coverage": "nyc -r lcov -e .ts -x \"*.test.ts\" npm run test",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "prepublish": "rm -rf dist/* && npm run build && cp manifest.json dist/ && cp styles.css dist/",
    "publish": "npm run version && npm run prepublish"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-typescript": "^6.0.0",
    "@tsconfig/svelte": "^1.0.13",
    "@types/chai": "^4.2.18",
    "@types/mocha": "^8.2.2",
    "chai": "^4.3.4",
    "cross-env": "^7.0.3",
    "ignore-styles": "^5.0.1",
    "jsdom": "^16.6.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^8.4.0",
    "nyc": "^15.1.0",
    "rollup": "^2.52.1",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-svelte": "^6.1.1",
    "svelte-check": "^1.6.0",
    "svelte-preprocess": "^4.7.3",
    "ts-node": "^9.1.1",
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "builtin-modules": "3.3.0",
    "esbuild": "0.14.47",
    "obsidian": "latest",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "svelte": "^3.38.2"
  }
}